#include <stdio.h>

int main() {
    float a, b;

    printf("Enter value of the first number: ");
    scanf("%f", &a);

    printf("Enter value of the second number: ");
    scanf("%f", &b);

    if (b != 0.0f) {
        printf("Result: %f\n", a / b);
    } else {
        printf("Error: Divisor can't be zero.\n");
    }

    return 0;
}
